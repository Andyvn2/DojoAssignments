<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>New Array</title>
    <script type="text/javascript">
    function numbersonly(arr){
      var newarray=[];
      for ( var i=0; i<arr.length; i++){
        if (typeof arr[i] === "number"){
          newarray.push(arr[i]);
        }
      }
      console.log(newarray)
    }

numbersonly([1, "apple", -3, "orange", 0.5])

    </script>
  </head>
  <body>

  </body>
</html>
